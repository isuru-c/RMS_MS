<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Student" table="student">
    <id name="id" type="string" column="id" length="8">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="firstName" type="string" column="first_name" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="secondName" type="string" column="second_name" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="faculty" type="string" column="faculty" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="department" type="string" column="department" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="gender" type="string" column="gender" length="6" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="birthday" type="date" column="birthday" nullable="false"/>
    <field name="contactNumber" type="string" column="contact_number" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="eMail" type="string" column="e_mail" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address" type="string" column="address" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
