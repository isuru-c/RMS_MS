{# student/assign #}

{% extends 'student/home.html.twig' %}

{% block about %}
{% endblock %}

{% block main %}

    {% if form is defined %}
        {% form_theme form 'bootstrap_3_horizontal_layout.html.twig' %}
        <div class="col-sm-11">
            <div class="well">
                {{ form_start(form) }}
                {{ form_widget(form) }}
                {{ form_end(form) }}
            </div>
        </div>
    {% else %}
        <div class="row">
            <div class="well success">
                {{ msg }}
            </div>
        </div>
    {% endif %}

{% endblock %}